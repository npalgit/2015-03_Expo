<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:hdc="http://schemas.hdcrd.com/xaml">
    <SolidColorBrush x:Key="{x:Static hdc:HdcBrushes.WindowBorderBrushKey}"
                     Color="#DDD" />
    <SolidColorBrush x:Key="{x:Static hdc:HdcBrushes.WindowBackgroundBrushKey}"
                     Color="#FFF" />
    <SolidColorBrush x:Key="{x:Static hdc:HdcBrushes.DialogBorderBrushKey}"
                     Color="#DDD" />
    <SolidColorBrush x:Key="{x:Static hdc:HdcBrushes.DialogBackgroundBrushKey}"
                     Color="#FFF" />
    <SolidColorBrush x:Key="{x:Static hdc:HdcBrushes.ForegroundBrushKey}"
                     Color="#FFF" />
    <SolidColorBrush x:Key="{x:Static hdc:HdcBrushes.BackgroundBrushKey}"
                     Color="#FF000000" />
    <SolidColorBrush x:Key="{x:Static hdc:HdcBrushes.BorderBrushKey}"
                     Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="{x:Static hdc:HdcBrushes.TransparentBrushKey}"
                     Color="#00FFFFFF" />
</ResourceDictionary>